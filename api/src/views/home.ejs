<body>
    <div class="d-flex" id="wrapper">
        <!-- Sidebar-->
        <div class="border-end" id="sidebar-fix">
            <div class="sidebar-heading border-bottom"><a class="list-group-item" href="/"><div class="icon-Header"></div></a></div>
            <div class="list-group list-group-flush">
                <div class="list-group">
                    <a class="list-group-item list-group-item-action text-center active" href="/">
                      <i class="fa-solid fa-house"></i>
                      <span style="font-size: 10px; display: block;">Dashboard</span>
                    </a>
                    <a class="list-group-item list-group-item-action text-center" href="/cars">
                      <i class="fa-solid fa-car-side"></i>
                      <span style="font-size: 10px; display: block;">Cars</span>
                    </a>
                  </div>
            </div>
        </div>
        <div class="border-end" id="sidebar-wrapper">
            <div class="sidebar-heading border-bottom"><div class="icon-Header"></div></div>
            <div class="list-group list-group-flush">
                <a class="list-group-item list-group-item-action list-group-item-light p-3 fw-medium disabled">DASHBOARD</a>
                <a class="list-group-item list-group-item-action list-group-item-light p-3 fw-medium active" href="/">Dashboard</a>
            </div>
        </div>
        <!-- Page content wrapper-->
        <div id="page-content-wrapper">
